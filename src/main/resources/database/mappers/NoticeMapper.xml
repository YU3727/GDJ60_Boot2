<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.pooh.base.board.notice.NoticeDAO">

	<!-- insert -->
	<insert id="setInsert" parameterType="com.pooh.base.board.BoardVO">
		INSERT INTO NOTICE(TITLE, CONTENTS, WRITER, REGDATE,  HIT)
		VALUES(#{title}, #{contents}, #{writer}, now(), 0)
	</insert>
  
</mapper>